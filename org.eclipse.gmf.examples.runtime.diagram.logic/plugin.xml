                   kind="org.eclipse.gmf.runtime.emf.type.core.IHintedType"> 
               <param name="semanticHint" value="XORGate"/>
           </metamodelType>
           <metamodelType
                   id="logic.terminal"     
                   eclass="Terminal"
                   edithelper="org.eclipse.gmf.examples.runtime.diagram.logic.internal.edithelpers.TerminalEditHelper"> 
